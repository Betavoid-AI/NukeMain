<script>
    // Get the button and div element
    const showDivButton = document.getElementById('addnu');
    const showDivButton2 = document.getElementById('newbunpro');
    const divElement = document.getElementById("makenewundleproject");
    const closeButton = divElement.querySelector(".closeee2");


    // Function to show the div and slide down animation
    function showDivAndSlideDown() {
        divElement.style.display = "block";
        divElement.style.animation = "slideDown 1s";
    }

    // Function to handle the closing animation and hide the div
    function closeDivAndSlideUp() {
        divElement.style.animation = "slideUp 1s";

        // Wait for the animation to finish and then set display to none
        setTimeout(() => {
            divElement.style.display = "none";
        }, 990); // 1000ms is the same duration as the slideUp animation
    }

    // Add click event listener to the button to show the div and slide down
    showDivButton.addEventListener("click", showDivAndSlideDown);

    if (!showDivButton2 == '') {
        showDivButton2.addEventListener("click", showDivAndSlideDown);   
    }

    // Add click event listener to the closeButton to close the div and slide up
    closeButton.addEventListener("click", closeDivAndSlideUp);
</script>

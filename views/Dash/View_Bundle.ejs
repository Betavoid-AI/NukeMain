<!-- submitting the form for new projects -->
    <script>
        const btnfornew = document.getElementById('submitbtnfornew');
        const newprojectform = document.getElementById('tehnewprojectform');


            
        //getting position of the nodes from the db ======================
        const positionofnodes = document.getElementById('allnodeposition').innerHTML;

        //converts string to object, for position of nodes
        function convertStringToObject(inputString) {

            const keyValuePattern = /(\w+)\s*:\s*'([^']+)'/g;
            const resultObject = {};

            let match;
            while ((match = keyValuePattern.exec(inputString)) !== null) {
                    const key = match[1];
                    const value = match[2];
                    resultObject[key] = value;
                }

            return resultObject;

        }     

        const positionofallnodes = convertStringToObject(positionofnodes);
        //getting position of the nodes from the db ======================



        //number of keys in the JSON
        const lengthofkeys = Object.keys(positionofallnodes).length;

        

        //adds new entry in the json
        function newprojectmade(){

            //adding fixed position for the new node ==============

            const idofnode = 'node' + (lengthofkeys+1);

            //getting latest location of the nodes
            const ntop = 100;
            const nleft = 960;

            //puttingion of the nodes in the position map
            positionofallnodes[idofnode] = [ntop, nleft];

            //adding fixed position for the new node ==============


            //putting thet hocation of the nodes into the update form
            const mappp = Object.entries(positionofallnodes)
            .map(([key, value]) => `${key}: '${value}'`)
            .join(', ');

            document.getElementById('Allnodes').innerHTML = mappp;
            //putting the map that has  o    des into the update form

        }


        btnfornew.addEventListener('click', () => {

            //pull position json from DB and update it
            newprojectmade();

            //Submites the position of the nodes form to the db
            setTimeout(function() {
                document.getElementById('updatebundleshit').submit();


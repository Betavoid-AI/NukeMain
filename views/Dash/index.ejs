<script>

  //when a bundle is to be liked
  function likedthisbundle(desc, title, allnodes, emogi, Allwire, id, index) {
    
    //not liked btn
    if (document.getElementById('notlikedbtndb'+index)) {
      document.getElementById('notlikedbtndb'+index).style.display = 'none';
    }
    if (document.getElementById('notlikedbtnh'+index)) {
      document.getElementById('notlikedbtnh'+index).style.display = 'none';
    }

    //like btn
    if (document.getElementById('likedbtnh'+index)) {
      document.getElementById('likedbtnh'+index).style.display = 'block';
    }
    if (document.getElementById('likedbtndb'+index)) {
      document.getElementById('likedbtndb'+index).style.display = 'block';
    }

    

    document.getElementById('titleob').innerHTML = title;
    document.getElementById('descriptionob').innerHTML = desc;
    document.getElementById('Allnodes').innerHTML = allnodes;
    document.getElementById('Allwire').innerHTML = Allwire;
    document.getElementById('emogi').innerHTML = emogi;

    document.getElementById('updatebundleshit').action = "/Bundle_Editorfromhome/"+id+"?_method=PUT";
    document.getElementById('updatebundleshit').submit();

    setTimeout(function() {
      window.stop();
    }, 2000);
  }

  //when a bundle is to be unliked
  function unlikedthisbundle(desc, title, allnodes, emogi, Allwire, id, index) {
    
    //not liked btn
    if (document.getElementById('notlikedbtndb'+index)) {
      document.getElementById('notlikedbtndb'+index).style.display = 'block';
    }
    if (document.getElementById('notlikedbtnh'+index)) {
      document.getElementById('notlikedbtnh'+index).style.display = 'block';
    }

    //like btn
    if (document.getElementById('likedbtnh'+index)) {
      document.getElementById('likedbtnh'+index).style.display = 'none';
    }
    if (document.getElementById('likedbtndb'+index)) {
      document.getElementById('likedbtndb'+index).style.display = 'none';
    }

    document.getElementById('titleob').innerHTML = title;
    document.getElementById('descriptionob').innerHTML = desc;
    document.getElementById('Allnodes').innerHTML = allnodes;
    document.getElementById('Allwire').innerHTML = Allwire;
    document.getElementById('emogi').innerHTML = emogi;

    document.getElementById('updatebundleshit').action = "/Bundle_Editorfromhome/"+id+"?_method=PUT";
    document.getElementById('updatebundleshit').submit();

    setTimeout(function() {
      window.stop();
    }, 2000);

  }


</script>
